<?xml version="1.0"?>
<package>
  <name>mppi_control</name>
  <version>0.0.0</version>
  
  <description>The mppi_control package facilitates autonomous navigation for the Jackal Robot, employing MPPI/log-MPPI/GP-MPPI based on the cost map generated by the onboard sensors.</description>

  <maintainer email="mohamedi@iu.edu">Ihab S. Mohamed</maintainer>
  <license>MIT</license>

  <buildtool_depend>catkin</buildtool_depend>
  <build_depend>geometry_msgs</build_depend>
  <build_depend>roscpp</build_depend>
  <build_depend>rospy</build_depend>
  <build_depend>std_msgs</build_depend>
  <build_depend>nav_msgs</build_depend>	
  <build_depend>message_generation</build_depend>

  <run_depend>geometry_msgs</run_depend>
  <run_depend>roscpp</run_depend>
  <run_depend>rospy</run_depend>
  <run_depend>std_msgs</run_depend>
  <run_depend>nav_msgs</run_depend>
  <run_depend>message_runtime</run_depend>
  <run_depend>ardrone_autonomy</run_depend>

  <export> </export>
</package>
